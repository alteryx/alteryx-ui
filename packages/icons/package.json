{
  "name": "@alteryx/icons",
  "version": "1.2.0",
  "description": "Alteryx React Icons",
  "main": "build/index.js",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ./.babelrc.js ./src --copy-files --out-dir ./build --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ./.babelrc.js ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node --config-file ./.babelrc.js ./scripts/copy-files.js",
    "build:copy-license": "babel-node --config-file ./.babelrc.js ./scripts/copy-files.js",
    "release": "npm run build:copy-files && npm publish build --non-interactive",
    "clean": "rimraf build",
    "compile": "node ./bin/build",
    "build": "npm run compile && npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files",
    "icons:watch": "babel --config-file ./.babelrc.js ./src --out-dir ./build --ignore *.test.js --watch",
    "watch": "run-p icons:watch",
    "lint": "eslint ./bin/** ./scripts/**"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.7.2",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "camelcase": "^6.0.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^7.0.0",
    "eslint": "^7.0.0",
    "fs-extra": "^9.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.0",
    "typescript": "^4.0.2",
    "uppercamelcase": "^3.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "license": "MIT"
}
